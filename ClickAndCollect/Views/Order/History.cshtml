@model List<ClickAndCollect.ViewModels.OrderTimeSlotOrderProductViewModel>

<div class="text-center">
    <table class="table">
        <tr>
            <th>Référence de la commande</th>
        </tr>
            @{
                foreach (var o in Model)
                {
                    <tr>
                        <td>@o.Order.OrderId</td>
                        <td><a asp-controller="Order" asp-action="Details" asp-route-OrderId="@o.Order.OrderId">Details</a></td>
                    </tr>
                }
            }


        </table>

</div>