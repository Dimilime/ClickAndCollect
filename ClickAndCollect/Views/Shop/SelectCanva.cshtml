@model List<TimeSlot>

<div class="alert-danger">
    @TempData["ErrorNbr"]
</div>

<div class="text-center">
    <table class="table">
        <tr>
            <th>Heure de début</th>
            <th>Heure de fin</th>
        </tr>
        @foreach (TimeSlot timeSlot in Model)
        {
        <tr>
            <td>@timeSlot.Start</td>
            <td>@timeSlot.End</td>

            <td><a asp-controller="TimeSlot" asp-action="GetCanva" asp-route-IdCanva="@timeSlot.IdCanva">Choisir</a></td>
        </tr>
        }
    </table>

</div>